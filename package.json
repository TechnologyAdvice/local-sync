{
  "name": "local-sync",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/local-sync.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "while read line; do rm -rf $line; done < .gitignore",
    "clean:build": "rm -rf dist",
    "lint": "eslint .",
    "lint:watch": "npm run watch -- 'npm run lint --silent'",
    "lint:fix": "npm run lint -- --fix",
    "watch": "DEBUG=watch babel-node scripts/watch.js",
    "webpack": "webpack --config webpack.config.babel.js",
    "prebuild": "npm run clean:build",
    "build": "npm run webpack && npm run build:production && npm run docs",
    "build:watch": "npm run watch -- 'npm run build'",
    "docs": "jsdoc2md --template src/README.hbs src/**/*.js > README.md",
    "build:production": "npm run webpack -- -p --output-filename ${npm_package_name}.min.js"
  },
  "pre-commit": ["lint"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechnologyAdvice/local-sync.git"
  },
  "author": "TechnologyAdvice <devteam@technologyadvice.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TechnologyAdvice/local-sync/issues"
  },
  "homepage": "https://github.com/TechnologyAdvice/local-sync#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "debug": "^2.2.0",
    "eslint-config-ta": "^3.1.1",
    "jsdoc-to-markdown": "^1.3.2",
    "pre-commit": "^1.1.2",
    "watch": "^0.17.1",
    "webpack": "^1.12.9"
  }
}
